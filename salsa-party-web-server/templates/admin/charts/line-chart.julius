const data#{rawJS classIdent} = {
  labels: #{labels},
  datasets: #{datasets},
};
const config#{rawJS classIdent} = {
  type: 'line',
  data: data#{rawJS classIdent},
  options: {
    indexAxis: 'x',
    scales: {
      y: {
        beginAtZero: true
      }
    }
  }
};

var ctx#{rawJS classIdent} = document.getElementById(#{classIdent});
var myChart#{rawJS classIdent} = new Chart(ctx#{rawJS classIdent}, config#{rawJS classIdent});
